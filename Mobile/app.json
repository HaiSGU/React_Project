{
  "expo": {
    "name": "Foodfastapp",
    "slug": "Foodfastapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../shared/assets/images/restaurant/foodfastlogo.jpg",
    "scheme": "foodfastapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "../shared/assets/images/restaurant/foodfastlogo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*",
      "../shared/assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Ứng dụng cần truy cập camera để quét QR thanh toán",
        "NSMicrophoneUsageDescription": "Ứng dụng cần truy cập microphone để ghi âm",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.nhathaisgu.Foodfastapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "../shared/assets/images/restaurant/foodfastlogo.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.nhathaisgu.Foodfastapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "../shared/assets/images/restaurant/foodfastlogo.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "../shared/assets/images/restaurant/foodfastlogo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d341fe3a-fcbd-41fc-93df-340589bea91e"
      }
    }
  }
}
